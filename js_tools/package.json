{
  "name": "recipes.riceprower.com",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run build:dev && onchange \"src/styles.css\" \"tailwind.config.js\" \"../*.html\" \"../*.md\" \"../_layouts/*.html\" \"../_layouts/*.md\" \"../_includes/*.html\" \"../_includes/*.md\" -- npm run build:dev",
    "build": "postcss ./src/**/*.css -o ../static/css/styles.css --env production",
    "build:dev": "postcss ./src/**/*.css -o ../static/css/styles.css",
    "build:prod": "postcss ./src/**/*.css -o ../static/css/styles.css --env production"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "autoprefixer": "^10.4.8",
    "material-ui-colors": "^1.0.0",
    "onchange": "^7.1.0",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^15.0.0",
    "postcss-minify": "^1.1.0",
    "tailwindcss": "^3.1.8"
  }
}
